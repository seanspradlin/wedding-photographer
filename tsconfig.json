{
    "compilerOptions": {
        "target": "es5",
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "moduleResolution": "node",
        "outDir": ".dist/app"
    },
    "exclude": [
        "node_modules",
        "typings",
        ".dist"
    ]
}