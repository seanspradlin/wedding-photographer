{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "vue"
    ],
    "env": {
      "browser": true,
      "jquery": true,
      "node": true
    },
    "globals": {
      "FB": true
    },
    "rules": {
      "no-new": 0,
      "strict": 0,
      "new-cap": 0,
      "no-shadow": 0,
      "no-underscore-dangle": 0
    }
}
